        "on": "ïƒ„ "
      },
      "random-icons": {
        "off": "<span color=\"#f53c3c\">ï´</span> ",
        "on": "ï´ "
      },
      "repeat-icons": {
        "on": "ï€ "
      },
      "single-icons": {
        "on": "ï€1 "
      },
      "state-icons": {
        "paused": "ïŒ",
        "playing": "ï‹"
      }
    },

    "battery": {
        "states": {
            // "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format": "{icon}",
        "format-full": ["ï‰€"],
        "format-charging": "ïƒ§",
        "format-plugged": "ïƒ§",
        "format-icons": ["ï‰„", "ï‰ƒ", "ï‰‚", "ï‰", "ï‰€"]
